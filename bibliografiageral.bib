@Article{HashimotoOrtegaCanchoCordeiro-logexponentiatedWeibull-2010,
  author    = {{E}lizabeth {M}. {H}ashimoto and {O}rtega, {E}dwin {M}oises {M}arcos and {V}icente {G}. {C}ancho and {C}ordeiro, {G}auss {M}outinho},
