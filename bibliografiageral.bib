@Article{LouzadaRamosNascimento-InverseNakagamim-2018,
  author    = {F. {Louzada} and P. L. {Ramos} and Nascimento, Diego},
  journal   = {IEEE Transactions on Reliability},
  pages     = {1030-1042},
  title     = {The Inverse Nakagami-m Distribution: A Novel Approach in Reliability},
  volume    = {67},
  year      = {2018},
  number    = {3},
  month     = {Sep.},
  abstract  = {In the paper, the inverse Nakagami-m (NK) distribution is proposed. This distribution is the reciprocal of the NK model that plays an important role in the general area of the communications engineering and reliability system. The proposed model is useful to describe devices that are subjected to high stress, providing high failure rate after a short repair time. An account of mathematical properties is presented such as the rth moment, mean, variance, rth central moment, survival properties, and Shannon's entropy. The maximum-likelihood estimators are explored under complete and censored data, but a bias correction was applied to the order O(n-2) for obtaining nearly unbiased performance. An efficient closed-form maximum a posteriori (MAP) estimator was also proposed. A simulation study compares the performance of the estimators with a clear advantage for the closed-form MAP one. Finally, we illustrate the new distribution fitting on three real datasets.},
  doi       = {10.1109/TR.2018.2829721},
  file      = {LouzadaRamosNascimento-InverseNakagamim-2018.pdf:L/LouzadaRamosNascimento-InverseNakagamim-2018.pdf:PDF},
  issn      = {0018-9529},
  keywords  = {maximum likelihood estimation;Nakagami channels;telecommunication network reliability;inverse Nakagami-m distribution;NK model;survival properties;Shannon's entropy;maximum-likelihood estimators;closed-form maximum a posteriori estimator;Maximum likelihood estimation;Reliability;Probability density function;Entropy;Nakagami distribution;Parameter estimation;Bias correction;censored data;inverse Nakagami-m (INK) distribution;maximum-likelihood estimators (MLEs);Nakagami-m (NK) distribution;reliability, PIBICTI},
  owner     = {supervedovatto},
  timestamp = {2019.08.08},
}

@Article{CronbachShavelson-MyCurrentThoughts-2004,
  author    = {Cronbach, Lee J. and Shavelson Richard J.},
  journal   = {Educational and Psychological Measurement},
  pages     = {391-418},
  title     = {My Current Thoughts on Coefficient Alpha and Successor Procedures},
  volume    = {64},
  year      = {2004},
  number    = {3},
  abstract  = {In 1997, noting that the 50th anniversary of the publication of “Coefficient Alpha and the Internal Structure of Tests” was fast approaching, Lee Cronbach planned what have become the notes published here. His aimwas to point out theways in which his views on coefficient alpha had evolved, doubting nowthat the coefficientwas the bestway of judging the reliability of an instrument to which it was applied. Tracing in these notes, in vintage Cronbach style, his thinking before, during, and after the publication of the alpha paper, his “current thoughts” on coefficient alpha are that alpha covers only a small perspective of the range of measurement uses for which reliability information is needed and that it should be viewed within a much larger system of reliability analysis, generalizability theory.},
  doi       = {10.1177/0013164404266386},
  eprint    = {https://doi.org/10.1177/0013164404266386},
