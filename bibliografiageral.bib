}

@Article{Figueiredo-Dynamicsregionalunemployment-2010,
  author    = {de {F}igueiredo, {E}rik {A}lencar},
  journal   = {Economic Modelling},
  pages     = {900--908},
  title     = {{D}ynamics of regional unemployment rates in {B}razil: {F}ractional behavior, structural breaks, and {M}arkov switching},
  volume    = {27},
  year      = {2010},
  number    = {5},
  month     = sep,
  abstract  = {This paper investigates unemployment dynamics in Brazil and in its major metropolitan regions using a fractional integration model. Aspects regarding structural breaks and regime switches are discussed as well. To do that, the methods proposed by Hassler and Meller (2009) and Tsay and Härdle (2009) are used. The major results indicate that unemployment rates have two different levels of persistence. The first one is nonstationary whereas the second one is nonstationary but mean-reverting. Based on these findings, the convergence hypothesis of regional unemployment rates was tested. Following the fractional stochastic convergence criterion put forward by Mello and Guimaraes-Filho (2007), it was concluded that regional unemployment rates are convergent.},
  keywords  = {Unemployment, Structural break, MS-ARFIMA process, Fractional stochastic convergence, Stochastic Processes},
  language  = {english},
  owner     = {supervedovatto},
  timestamp = {2014.05.04},
  url       = {http://ideas.repec.org/a/eee/ecmode/v27y2010i5p900-908.html},
}

