  volume    = {5},
  year      = {2016},
  number    = {1},
  abstract  = {A new six-parameter extended fatigue lifetime model named the McDonald-Burr XII distribution is introduced, which generalizes the Burr XII, beta Burr XII (Parana\'iba {\it et al.}, 2011) and Kumaraswamy-Burr XII (Parana\'iba {\it et al.}, 2012) distributions. The proposed distribution is characterized in terms of truncated moments. We obtain the ordinary and incomplete moments and quantile and ge\-ne\-ra\-ting functions. We estimate the model parameters by maximum likelihood. A Monte Carlo simulation is performed to study the asymptotic normality of the estimates. We also propose an extended regression model based on the logarithm of the McDonald-Burr XII distribution that can be more realistic in the analysis of real data than other special regression models. Two applications to real data validate the importance of the new models.},
  file      = {CordeiroOrtegaHamedaniGarcia-McBurrXIIand-2016-previa.pdf:C/CordeiroOrtegaHamedaniGarcia-McBurrXIIand-2016-previa.pdf:PDF;CordeiroOrtegaHamedaniGarcia-McBurrXIIand-2016.pdf:C/CordeiroOrtegaHamedaniGarcia-McBurrXIIand-2016.pdf:PDF},
  groups    = {Cauchy-Rayleigh, 7-parameters, McDonald (Generalized-Beta), Probability Models, Regression Analysis},
  issn      = {1927-7040},
  keywords  = {Cauchy-Rayleigh, Probability distribution, 3-parameters, Probability Generator Functions, Probability Generator Family},
  language  = {english},
  owner     = {supervedovatto},
  timestamp = {2017.01.18},
  url       = {http://www.ccsenet.org/journal/index.php/ijsp/article/view/51846},
}

@Article{CordeiroOrtegaLemonte-exponentialWeibulllifetime-2014,
  author    = {{C}ordeiro, {G}auss {M}outinho and {O}rtega, {E}dwin {M}oises {M}arcos and {L}emonte, {A}rtur {J}ose},
  journal   = {Journal of Statistical Computation and Simulation},
  pages     = {2592--2606},
