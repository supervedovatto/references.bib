@Article{DavidsonHashimzade-TypeIand-2009,
  author      = {{D}avidson, {J}ames and {H}ashimzade, {N}igar},
  journal     = {Computational Statistics \& Data Analysis},
  pages       = {2089--2106},
  title       = {{T}ype {I} and type {II} fractional {B}rownian motions: A reconsideration},
  volume      = {53},
  year        = {2009},
  number      = {6},
  month       = apr,
  institution = {Exeter University, Department of Economics},
  abstract    = {The so-called type I and type II fractional Brownian motions are limit distributions associated with the fractional integration model in which pre-sample shocks are either included in the lag structure, or suppressed. There can be substantial differences between the distributions of these two processes and of functionals derived from them, so that it becomes an important issue to decide which model to use as a basis for inference. Alternative methods for simulating the type I case are contrasted, and for models close to the nonstationarity boundary, truncating infinite sums is shown to result in a significant distortion of the distribution. A simple simulation method that overcomes this problem is described and implemented. The approach also has implications for the estimation of type I ARFIMA models, and a new conditional ML estimator is proposed, using the annual Nile minima series for illustration.},
  keywords    = {Fractional Brownian motion, long memory, ARFIMA, simulation.},
  language    = {english},
  owner       = {supervedovatto},
  timestamp   = {2014.08.30},
  type        = {Discussion Papers},
  url         = {http://ideas.repec.org/a/eee/csdana/v53y2009i6p2089-2106.html},
}

@Article{DavidsonSibbertsen-Generatingschemeslong-2005,
  author      = {{D}avidson, {J}ames and {S}ibbertsen, {P}hilipp},
  journal     = {Journal of {E}conometrics},
  pages       = {253--282},
  title       = {Generating schemes for long memory processes: regimes, aggregation and linearity},
  volume      = {128},
  year        = {2005},
  number      = {2},
  month       = oct,
  institution = {Technische Universität Dortmund, Sonderforschungsbereich 475: Komplexitätsreduktion in multivariaten Datenstrukturen},
  abstract    = {This paper analyses a class of nonlinear time series models exhibiting long memory. These processes exhibit short memory fluctuations around a local mean (regime) which switches randomly such that the durations of the regimes follow a power law. We show that if a large number of independent copies of such a process are aggregated, the resulting processes are Gaussian, have a linear representation, and converge after normalisation to fractional Brownian motion. Two cases arise, a stationary case in which the partial sums of the process converge, and a nonstationary case in which the process itself converges, the Hurst coefficient falling in the ranges ( 1 2 , 1) and (0, 1 2 ) respectively. However, a non-aggregated regime process is shown to converge to a Levy motion with infinite variance, suitably normalised, emphasising the fact that time aggregation alone fails to yield a FCLT. We comment on the relevance of our results to the interpretation of the long memory phenomenon, and also report some simulations aimed to throw light on the problem of discriminating between the models in practice. --<P>(This abstract was borrowed from another version of this item.)},
  language    = {english},
  owner       = {supervedovatto},
  timestamp   = {2014.09.27},
  type        = {Technical Reports},
  url         = {http://ideas.repec.org/a/eee/econom/v128y2005i2p253-282.html},
}

