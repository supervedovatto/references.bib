@Article{EmekterJirasakuldechSnaith-Nonlineardynamicsin-2009,
  author    = {{E}mekter, {R}iza and {J}irasakuldech, {B}enjamas and {S}naith, {S}ean {M}.},
  journal   = {Journal of Multinational Financial {M}anagement},
  pages     = {179--192},
  title     = {Nonlinear dynamics in foreign exchange excess returns: Tests of asymmetry},
  volume    = {19},
  year      = {2009},
  number    = {3},
  month     = jul,
  abstract  = {This paper examines the nonlinear dynamic behaviors in foreign exchange excess returns of eight currencies. We applied the BDS test and two other nonlinear statistical techniques, the Markov chain, and time reversibility tests to characterize the exchange rate returns dynamics. The results from the BDS test provide strong evidence of nonlinear dependence on the British pound, the Singapore dollar, the South African rand, and the Swedish krone. The Markov chain test shows evidence of a non-random walk and positive serial dependence in all currencies except for the British pound, the Canadian dollar, and the Swiss franc. Lastly, evidence of time irreversible and asymmetric dynamic behavior is found in seven currencies with the exception of the Canadian dollar. The results indicate that the asymmetry in the Singapore dollar, the South African rand, and the Swiss franc is due to nonlinearity in the functional form as opposed to non-Gaussian innovations.},
  keywords  = { Exchange rate Nonlinear dynamics Asymmetry},
  language  = {english},
  owner     = {supervedovatto},
  timestamp = {2014.05.04},
  url       = {http://ideas.repec.org/a/eee/mulfin/v19y2009i3p179-192.html},
}

