  keywords  = {Gamma distribution, Weibull distribution, Likelihood ratio ordering, Hazard rate ordering, Stochastic ordering, Fisher Information matrix, Maximum Likelihood Estimator, Haghighi, Continuous distribution, Probability distribution, Exponentiated Exponential, Exponentiated Distributions, 1-parameter, Probability Generator Functions, Probability Generator Family},
  language  = {english},
  owner     = {supervedovatto},
  publisher = {WILEY-VCH Verlag Berlin GmbH},
  subtitle  = {An Alternative to Gamma and {W}eibull Distributions},
  timestamp = {2016.03.23},
