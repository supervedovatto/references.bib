@Article{GraneroSegoviaPerez-SomeCommentsHurst-2008,
  author    = {{M}.{A}. {S}\'anchez {G}ranero and {J}.{E}. {T}rinidad {S}egovia and {J}. {G}arc{\'\i}a {P}\'erez},
