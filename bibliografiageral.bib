  keywords      = {EM algorithm, Expectation-Maximization, Cauchy-Rayleigh, Probability distribution, Mixture of distributions, Distributions discrimination},
  owner         = {supervedovatto},
  publisher     = {Society for Industrial and Applied Mathematics},
