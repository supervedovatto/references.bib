  keywords      = {Mixture of two Burr type XII, Identifiability, Classification rules, Probability of misclassification, Cauchy-Rayleigh},
  owner         = {supervedovatto},
  timestamp     = {2018.08.24},
  url           = {http://www.sciencedirect.com/science/article/pii/0898122194001367},
}

