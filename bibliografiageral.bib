@Article{CordeiroOrtegaHamedaniGarcia-McBurrXIIand-2016,
  author    = {{C}ordeiro, {G}auss {M}outinho and {O}rtega, {E}dwin {M}oises {M}arcos and {H}amedani, {G}holamhossein {G}. and {G}arcia, {D}iogo {A}.},
