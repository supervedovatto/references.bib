@Article{MazucheliMenezesDey-Biascorrectedmaximum-2018,
  author        = {{M}azucheli, {J}osmar and {M}enezes, {A}ndr\'e {F}elipe {B}erdusco and {D}ey, {S}anku},
  journal       = {Communications in Statistics - Simulation and Computation},
  pages         = {1--10},
  title         = {{B}ias-corrected maximum likelihood estimators of the parameters of the inverse {W}eibull distribution},
  volume        = {0},
  year          = {2018},
  number        = {0},
  __markedentry = {[supervedovatto:]},
  abstract      = { ABSTRACTMaximum likelihood estimators usually have biases of the order for large sample size n which are very often ignored because of the fact that they are small when compared to the standard errors of the parameter estimators that are of order . The accuracy of the estimates may be affected by such bias. To reduce such bias of the MLEs from order to order , we adopt some bias-corrected techniques. in this paper, we adopt two approaches to derive first-order bias corrections for the the maximum likelihood estimators of the parameters of the Inverse Weibull distribution. The first one is the analytical methodology suggested by Cox and Snell (1968) and the second is based on the parametric Bootstrap resampling method. Monte Carlo simulations are conducted to investigate the performance of these methodologies. Our results reveal that the bias corrections improve the accuracy as well as the consistency of the estimators. Finally, an example with a real data set is presented. },
  doi           = {10.1080/03610918.2018.1433838},
  eprint        = {https://doi.org/10.1080/03610918.2018.1433838},
  file          = {MazucheliMenezesDey-Biascorrectedmaximum-2018.pdf:M/MazucheliMenezesDey-Biascorrectedmaximum-2018.pdf:PDF},
  groups        = {Cox-Snell, Cauchy-Rayleigh},
  keywords      = {Bias Correction, Cox-Snell Correction, Cauchy-Rayleigh},
  owner         = {supervedovatto},
  publisher     = {Taylor \& Francis},
  timestamp     = {2019.01.05},
  url           = { 
 https://doi.org/10.1080/03610918.2018.1433838
 
},
}

