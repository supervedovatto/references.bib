}

@Article{RodriguesCordeiroCanchoBalakrishnan-RelaxedPoissoncure-2015,
  author    = {{R}odrigues, {J}osemar and {C}ordeiro, {G}auss {M}. and {C}ancho, {V}icente {G}. and {B}alakrishnan, {N}.},
  journal   = {Biometrical Journal},
  pages     = {n/a-n/a},
  title     = {{R}elaxed {P}oisson cure rate models},
  year      = {2015},
  abstract  = {The purpose of this article is to make the standard promotion cure rate model (Yakovlev and Tsodikov, ) more flexible by assuming that the number of lesions or altered cells after a treatment follows a fractional Poisson distribution (Laskin, ). It is proved that the well-known Mittag-Leffler relaxation function (Berberan-Santos, ) is a simple way to obtain a new cure rate model that is a compromise between the promotion and geometric cure rate models allowing for superdispersion. So, the relaxed cure rate model developed here can be considered as a natural and less restrictive extension of the popular Poisson cure rate model at the cost of an additional parameter, but a competitor to negative-binomial cure rate models (Rodrigues et al., ). Some mathematical properties of a proper relaxed Poisson density are explored. A simulation study and an illustration of the proposed cure rate model from the Bayesian point of view are finally presented.},
  doi       = {10.1002/bimj.201500051},
  file      = {RodriguesCordeiroCanchoBalakrishnan-RelaxedPoissoncure-2015.pdf:R/RodriguesCordeiroCanchoBalakrishnan-RelaxedPoissoncure-2015.pdf:PDF},
  issn      = {1521-4036},
  keywords  = {Bayesian inference, Fractional Poisson distribution, Geometric cure rate model, Mittag-Leffler relaxation function, Poisson cure rate model, Relaxed Poisson cure rate model},
  owner     = {supervedovatto},
  timestamp = {2016.02.26},
  url       = {http://dx.doi.org/10.1002/bimj.201500051},
}

