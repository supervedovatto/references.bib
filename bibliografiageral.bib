@Article{JondeauSahuc-OptimalMonetaryPolicy-2008,
  author    = {{E}ric {J}ondeau and {J}ean-{G}uillaume {S}ahuc},
  journal   = {International Journal of {C}entral {B}anking},
  pages     = {23--72},
  title     = {{O}ptimal {M}onetary {P}olicy in an Estimated {DSGE} Model of the {E}uro {A}rea with {C}ross-{C}ountry {H}eterogeneity},
  volume    = {4},
  year      = {2008},
  number    = {2},
  month     = jun,
  abstract  = {This paper investigates the implications of cross-country heterogeneity within the euro area for the design of optimal monetary policy. We build an optimization-based multicountry model (MCM) describing the euro area in which differences between structural parameters across countries are allowed. Using Bayesian techniques, we estimate the MCM and its areawide counterpart (AWM).We then question which model is the most appropriate for monetary policy purposes. Several results emerge. First, using an AWM induces relatively large and significant welfare losses. Second, this is not the use of a rule based on aggregated variables that is costly in terms of welfare, but rather the use of a suboptimal forecasting model. Third, allowing for habit on consumption has important implications for the dynamics of models, but taking into account differences in price indexation has more drastic effects on welfare losses.},
  language  = {english},
  owner     = {supervedovatto},
  timestamp = {2014.05.04},
  url       = {http://ideas.repec.org/a/ijc/ijcjou/y2008q2a2.html},
}

@Techreport{JonesLamontLumsdaine-PublicInformationand-NBoERI-1996,
  author      = {{C}harles {M}. {J}ones and {O}wen {L}amont and {R}obin {L}umsdaine},
  institution = {National Bureau of Economic Research, Inc},
  title       = {Public Information and the {P}ersistence of {B}ond {M}arket {V}olatility},
  year        = {1996},
  type        = {NBER Working Papers},
  number      = {5446},
  month       = {\#Jan\#},
  abstract    = {We examine the reaction of daily bond prices to the release of government macroeconomic news. These news releases are of interest because they are released on periodic, preannounced dates and because they cause substantial bond market volatility. The news component of volatility is not positively autocorrelated on these dates, since the news is released at a specific moment in time. We find that (1) expected returns on the short end of the bond market are significantly higher on these announcement dates, and (2) the persistence pattern of daily volatility is quite different around these days.},
  language    = {english},
  owner       = {supervedovatto},
  timestamp   = {2014.05.04},
  url         = {http://ideas.repec.org/p/nbr/nberwo/5446.html},
}

@Article{Jones-Kumaraswamy’sdistribution:beta-2009,
  author    = {{M}.{C}. {J}ones},
  journal   = {Statistical Methodology},
  pages     = {70--81},
  title     = {{K}umaraswamy’s distribution: A beta-type distribution with some tractability advantages},
  volume    = {6},
  year      = {2009},
  number    = {1},
  abstract  = {A two-parameter family of distributions on ( 0 , 1 ) is explored which has many similarities to the beta distribution and a number of advantages in terms of tractability (it also, of course, has some disadvantages). Kumaraswamy’s distribution has its genesis in terms of uniform order statistics, and has particularly straightforward distribution and quantile functions which do not depend on special functions (and hence afford very easy random variate generation). The distribution might, therefore, have a particular role when a quantile-based approach to statistical modelling is taken, and its tractability has appeal for pedagogical uses. To date, the distribution has seen only limited use and development in the hydrological literature.},
  doi       = {http://dx.doi.org/10.1016/j.stamet.2008.04.001},
  groups    = {Kumaraswamy type I},
