  timestamp = {2017.05.21},
  url       = {http://dx.doi.org/10.1186/s40488-016-0043-2},
}

@Article{CordeiroLimaCysneirosPascoaPescimOrteg-extendedBirnbaum–Saundersdistribution:-2016,
  author    = {{G}auss {M}. {C}ordeiro and {M}aria {D}o {C}armo {S}. {L}ima and {A}udrey {H}.{M}.{A}. {C}ysneiros and {M}arcelino {A}. {R}. {P}ascoa and {R}odrigo {R}. {P}escim and {E}dwin {M}. {M}. {O}rtega},
  journal   = {Communications in Statistics - Theory and Methods},
  pages     = {2268--2297},
  title     = {An extended {B}irnbaum–{S}aunders distribution: Theory, estimation, and applications},
  volume    = {45},
  year      = {2016},
  number    = {8},
  abstract  = { AbstractBirnbaum and Saunders (1969a) pioneered a lifetime model which is commonly used in reliability studies. Based on this distribution, a new model called the gamma Birnbaum–Saunders distribution is proposed for describing fatigue life data. Several properties of the new distribution including explicit expressions for the ordinary and incomplete moments, generating and quantile functions, mean deviations, density function of the order statistics, and their moments are derived. We discuss the method of maximum likelihood and a Bayesian approach to estimate the model parameters. The superiority of the new model is illustrated by means of three failure real data sets. We also propose a new extended regression model based on the logarithm of the new distribution. The last model can be very useful to the analysis of real data and provide more realistic fits than other special regression models. },
  doi       = {10.1080/03610926.2013.879182},
  eprint    = {http://dx.doi.org/10.1080/03610926.2013.879182},
  file      = {CordeiroLimaCysneiros-extendedBirnbaumSaunders-2016.pdf:C/CordeiroLimaCysneiros-extendedBirnbaumSaunders-2016.pdf:PDF},
  groups    = {Birnbaum–Saunders power series, Regression Analysis},
  keywords  = {Birnbaum-Saunders distribution, Fatigue life distribution, Gamma distribution, Lifetime data, Maximum likelihood estimation, Regression model, Methods to generate distributions, Probability Generator Functions, Probability Generator Family},
  owner     = {supervedovatto},
  subtitle  = {Theory, estimation and applications},
  timestamp = {2017.04.18},
  url       = { http://dx.doi.org/10.1080/03610926.2013.879182},
}

