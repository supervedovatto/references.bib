@Article{SilvaOrtegaCanchoBarreto-LogBurrXII-2008,
  author    = {{S}ilva, {G}iovana {O}liveira and {O}rtega, {E}dwin {M}oises {M}arcos and {C}ancho, {V}icente {G}aribay and {B}arreto, {M}auricio {L}ima},
  journal   = {Computational Statistics \& Data Analysis},
  pages     = {3820--3842},
  title     = {Log-{B}urr {XII} regression models with censored data},
  volume    = {52},
  year      = {2008},
  number    = {7},
  abstract  = {In survival analysis applications, the failure rate function may frequently present a unimodal shape. in such case, the log-normal or log-logistic distributions are used. in this paper, we shall be concerned only with parametric forms, so a location-scale regression model based on the Burr XII distribution is proposed for modeling data with a unimodal failure rate function as an alternative to the log-logistic regression model. Assuming censored data, we consider a classic analysis, a Bayesian analysis and a jackknife estimator for the parameters of the proposed model. For different parameter settings, sample sizes and censoring percentages, various simulation studies are performed and compared to the performance of the log-logistic and log-Burr XII regression models. Besides, we use sensitivity analysis to detect influential or outlying observations, and residual analysis is used to check the assumptions in the model. Finally, we analyze a real data set under log-Burr XII regression models.},
  date      = {2008-01-17},
  doi       = {http://dx.doi.org/10.1016/j.csda.2008.01.003},
  file      = {SilvaOrtegaCanchoBarreto-LogBurrXII-2008.pdf:S/SilvaOrtegaCanchoBarreto-LogBurrXII-2008.pdf:PDF},
  groups    = {Abraão, Cauchy-Rayleigh, Artigos, Modelos de Regressão, Regression Analysis},
  issn      = {0167-9473},
  keywords  = {Cauchy-Rayleigh, Time series, Regression model},
  owner     = {supervedovatto},
  timestamp = {2017.05.15},
  url       = {http://www.sciencedirect.com/science/article/pii/S0167947308000078},
}

