@Article{TahirZubairCordeiroAlzaatrehMansoor-PoissonXfamily-2015,
  author    = {{T}ahir, {M}uhammad {H}. and {Z}ubair, {M}uhammad and {C}ordeiro, {G}auss {M}outinho and {A}lzaatreh, {A}yman and {M}ansoor, {M}uhammad},
