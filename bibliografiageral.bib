@Article{SantanaOrtegaCordeiroSilva-KumaraswamyLogLogistic-2012,
  author    = {{S}antana, {T}iago {V}iana {Flor de} and {O}rtega, {E}dwin {M}oises {M}arcos and {C}ordeiro, {G}auss {M}outinho and {S}ilva, {G}iovana {O}liveira},
  journal   = {Journal of Statistical Theory and Applications},
  pages     = {265--291},
  title     = {The {K}umaraswamy Log-Logistic Distribution},
  volume    = {11},
  year      = {2012},
  number    = {3},
  abstract  = {The log-logistic distribution is widely used in survival analysis when the failure rate function presents a unimodal shape. Based on the log-logistic and Kumaraswamy distributions, we introduce the called Kumaraswamy-log-logistic distribution. The new distribution contains several important distributions discussed in the literature as sub-models such as the log-logistic, exponentiated log-logistic and Burr XII distributions, among several others. The beauty and importance of the new distribution lies in its ability to model non-monotonic failure rate functions, which are quite common in lifetime data analysis and reliability. Some of its structural properties are studied. We propose the Kumaraswamy-logistic regression model which has, as sub-models, various widely-known regression models. We discuss the method of maximum likelihood to estimate the model parameters and determine the observed information matrix. Two real data sets illustrate the importance and flexibility of the proposed models.},
  file      = {SantanaOrtegaCordeiroSilva-KumaraswamyLogLogistic-2012.pdf:S/SantanaOrtegaCordeiroSilva-KumaraswamyLogLogistic-2012.pdf:PDF},
  groups    = {Cauchy-Rayleigh, Log-Logistica (Generalizações), Kumaraswamy type I, Regression Analysis},
  issn      = {1538-7887},
  keywords  = {Censored data, Log-logistic distribution, Maximum likelihood estimation, Moment, Order statistic, Regression model., Cauchy-Rayleigh, Log-logistic generalization, 2-parameters, Kumaraswamy-G, Probability Generator Functions, Probability Generator Family},
