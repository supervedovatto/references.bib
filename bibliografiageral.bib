  owner         = {supervedovatto},
  timestamp     = {2018.04.15},
  url           = {http://www.sciencedirect.com/science/article/pii/0047259X82900653},
}

@Article{BibalanAmindavar-NonGaussianamplitude-2016,
  author        = {{B}ibalan, {M}ohammadreza {H}assannejad and {A}mindavar, {H}amidreza},
  journal       = {{EURASIP} Journal on Image and Video Processing},
  pages         = {48--50},
  title         = {Non-{G}aussian Amplitude PDF Modeling of Ultrasound Images Based on a Novel Generalized {C}auchy-{R}ayleigh Mixture},
  year          = {2016},
  number        = {1},
  __markedentry = {[supervedovatto:]},
  abstract      = {In this paper, a mixture of generalized Cauchy distribution and Rayleigh distribution that possesses a closed-form expression is proposed for modeling the heavy-tailed Rayleigh (HTR) distribution. This new approach is developed for analytically modeling the amplitude distribution of ultrasound images based on the HTR distribution. HTR as a non-Gaussian distribution is basically the amplitude probability density function (PDF) of the complex isotropic symmetric $\alpha$-stable (S $\alpha$ S) distribution which appears in the envelope distribution of ultrasonic images. Analytic expression for HTR distribution is a momentous consideration in signal processing with stable random variables. Furthermore, we introduce a mixture ratio estimator based on the energy of amplitude PDF which contains both $\alpha$ and $\gamma$ parameters. For a quantitative assessment, we compare the accuracy and computational complexity of the proposed mixture with other approximations of HTR distribution through several numeral simulations on synthetic random samples. Experimental results obtained from the Kolmogorov-Smirnov (K-S) distance and Kullback-Leibler (K-L) divergence as the goodness-of-fit tests on real ultrasound images reveal the favor of the new mixture model.},
  doi           = {10.1186/s13640-016-0148-z},
  file          = {BibalanAmindavar-NonGaussianamplitude-2016.pdf:B/BibalanAmindavar-NonGaussianamplitude-2016.pdf:PDF},
  groups        = {Cauchy-Rayleigh},
  issn          = {1687-5281},
  keywords      = {Cauchy-Rayleigh},
  owner         = {supervedovatto},
  timestamp     = {2017.05.15},
  url           = {http://dx.doi.org/10.1186/s13640-016-0148-z},
}

@Inproceedings{BibalanAmindavar-newalphaand-2015,
  author    = {{M}. {H}. {B}ibalan and {H}. {A}mindavar},
  booktitle = {{IEEE} International Conference on Acoustics, Speech and Signal Processing},
  title     = {A New Alpha and Gamma Based Mixture Approximation for Heavy-Tailed {R}ayleigh Distribution},
  year      = {2015},
  pages     = {3711--3715},
  address   = {Brisbane, Australia},
  month     = apr,
  publisher = {IEEE},
  abstract  = {In this paper, a novel bi-parameter mixture approximation for modeling the statistical properties of heavy-tailed Rayleigh distribution is developed. Heavy-tailed Rayleigh distribution is basically the amplitude PDF of bi-variate isotropic alpha-stable distribution which appears in the envelope distribution of impulsive interferences and SAR return signals. Closed-form expression for PDF is a serious consideration in signal processing with alpha-stable distribution. in this case, mixture approximation by means of Rayleigh and a generalization of Cauchy distribution is a simple but comprehensive approach. Several models try to give a model for the mixture ratio based on alpha. We propose two new methods for mixture ratio estimation based on both alpha and γ. Numerical results verify that the proposed mixture approximation is closer to the theoretical and empirical heavy-tailed Rayleigh distribution.},
  doi       = {10.1109/ICASSP.2015.7178664},
  file      = {BibalanAmindavar-newalphaand-2015.pdf:B/BibalanAmindavar-newalphaand-2015.pdf:PDF},
  groups    = {Cauchy-Rayleigh},
  issn      = {1520-6149},
  keywords  = {gamma distribution, mixture models, signal denoising, Cauchy distribution generalization, PDF, SAR return signal processing, alpha mixture approximation, biparameter mixture approximation, bivariate isotropic alpha-stable distribution, bivariate isotropic γ-stable distribution, gamma mixture approximation, heavy-tailed Rayleigh distribution, impulsive interference envelope distribution, statistical properties, Approximation methods, Distribution functions, Estimation, Mathematical model, Numerical models, Parameter estimation, characteristic exponent, dispersion, mixture approximation, symmetric alpha-stable, Cauchy-Rayleigh},
  owner     = {supervedovatto},
  timestamp = {2016.05.26},
}

