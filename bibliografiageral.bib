@Article{Gil-AlanaCunadoGraci-TourisminCanary-2008,
  author    = {{L}uis {A}. {G}il-{A}lana and {J}uncal {C}unado and {F}ernando {P}erez de {G}racia},
  journal   = {Journal of {F}orecasting},
  pages     = {621--636},
  title     = {{T}ourism in the {C}anary {I}slands: forecasting using several seasonal time series models},
  volume    = {27},
  year      = {2008},
  number    = {7},
  abstract  = { This paper deals with the analysis of the number of tourists travelling to the Canary Islands by means of using different seasonal statistical models. Deterministic and stochastic seasonality is considered. For the latter case, we employ seasonal unit roots and seasonally fractionally integrated models. As a final approach, we also employ a model with possibly different orders of integration at zero and the seasonal frequencies. All these models are compared in terms of their forecasting ability in an out-of-sample experiment. The results in the paper show that a simple deterministic model with seasonal dummy variables and AR(1) disturbances produce better results than other approaches based on seasonal fractional and integer differentiation over short horizons. However, increasing the time horizon, the results cannot distinguish between the model based on seasonal dummies and another using fractional integration at zero and the seasonal frequencies. Copyright © 2008 John Wiley \& Sons, Ltd.},
  language  = {english},
  owner     = {supervedovatto},
  timestamp = {2014.05.04},
  url       = {http://ideas.repec.org/a/jof/jforec/v27y2008i7p621-636.html},
}

@Techreport{Gil-AlanaFischer-Internationaltravelingand-EAoAE-2007,
  author      = {{G}il-{A}lana, {L}uis {A}. and {F}ischer, {C}hristian},
  institution = {European Association of Agricultural Economists},
  title       = {International traveling and trade: further evidence for the case of {S}panish wine based on fractional {VAR} specifications},
  year        = {2007},
  type        = {105th Seminar, March 8-10, 2007, Bologna, Italy},
  number      = {7859},
  abstract    = { This paper deals with the relationship between international travelling and trade. For this purpose we focus on a particular case study: the connection between the Spanish wine exports to Germany and the German travellers to Spain. Unlike previous studies we use a methodology based on fractional vector autoregressive models, which permits us to compute the impulse responses in a similar way as in the standard VAR case. The results show that the orders of integration of the two series are constrained between 0 and 1, being higher for the arrivals series than for the exports. The impulse response analysis reveals that an increase in travelling produces a positive initial impact on trade though it tends to disappear in the long run.},
  keywords    = {International trade, Multivariate models, Fractional VAR, Tourism., International Relations/Trade},
  language    = {english},
  owner       = {supervedovatto},
  timestamp   = {2014.05.04},
  url         = {http://ideas.repec.org/p/ags/eaa105/7859.html},
}

@Techreport{Gil-AlanaHenry-FractionalIntegrationand-SoEaBAUoN-2003,
  author      = {{L}uis {A}. {G}il-{A}lana and {S}.{G}. {B}rian {H}enry},
  institution = {School of Economics and Business Administration, University of Navarra},
  title       = {{F}ractional {I}ntegration and the {D}ynamics of {UK} {U}nemployment},
  year        = {2003},
  type        = {Faculty Working Papers},
  number      = {10/03},
  month       = {\#May\#},
  abstract    = {This article is concerned with the dynamic behaviour of UK unemployment. However, instead of using traditional approaches based on I(0) stationary or I(1) (integrated and/or cointegrated) models, we use the fractional integration framework. in doing so, we allow for a more careful study of the low frequency dynamics underlying the series. The conclusions suggest that the UK unemployment may be explained in terms of lagged values of the real oil prices and the real interest rate, with the order of integration of unemployment ranging between 0.50 and 1. Thus, unemployment shows the characteristics of long memory but is mean reverting.},
  journal     = {{O}xford {B}ulletin of Economics and Statistics},
  language    = {english},
